{
  "name": "graasp-player",
  "version": "0.6.3",
  "private": true,
  "author": "Graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang",
    "Basile Spaenlehauer",
    "Alexandre Chau",
    "Julien Torrent"
  ],
  "scripts": {
    "dev": "vite",
    "start:test": "vite --mode test",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "prettier:check": "prettier --check {src,cypress}/**/*.{js,ts,tsx}",
    "prettier:write": "prettier --write {src,cypress}/**/*.{js,ts,tsx}",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "test": "concurrently -k -s first \"yarn start:test\" \"yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --browser chrome"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@graasp/chatbox": "1.2.1",
    "@graasp/query-client": "0.4.1",
    "@graasp/sdk": "0.12.0",
    "@graasp/translations": "1.12.0",
    "@graasp/ui": "2.5.0",
    "@mui/icons-material": "5.11.9",
    "@mui/lab": "5.0.0-alpha.120",
    "@mui/material": "5.11.9",
    "@sentry/react": "7.50.0",
    "@sentry/tracing": "7.50.0",
    "clsx": "1.2.1",
    "immutable": "4.3.0",
    "lodash.truncate": "4.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "2.0.0",
    "react-i18next": "12.1.5",
    "react-intersection-observer": "9.4.3",
    "react-router": "6.8.1",
    "react-router-dom": "6.8.1",
    "react-toastify": "9.1.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.4.4",
    "@cypress/code-coverage": "3.10.0",
    "@graasp/plugin-websockets": "1.0.0",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/lodash.truncate": "^4.4.7",
    "@types/node": "^18.13.0",
    "@types/qs": "6.9.7",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "commitlint": "17.4.4",
    "cypress": "12.7.0",
    "cypress-iframe": "1.0.1",
    "cypress-vite": "1.3.1",
    "env-cmd": "10.1.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "http-status-codes": "2.2.0",
    "husky": "8.0.3",
    "path": "0.12.7",
    "prettier": "^2.8.4",
    "qs": "6.11.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-checker": "^0.5.5",
